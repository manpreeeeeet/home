---
export interface Props {
  title?: string;
}
const { title } = Astro.props;
---

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <!-- Global Metadata -->
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width,initial-scale=1" />
  <meta name="generator" content={Astro.generator} />

  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" href="/favicon.png" />
  <title>{title}</title>
</head>
